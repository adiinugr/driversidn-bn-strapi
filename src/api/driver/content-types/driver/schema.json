{
  "kind": "collectionType",
  "collectionName": "drivers",
  "info": {
    "singularName": "driver",
    "pluralName": "drivers",
    "displayName": "Driver",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "language": {
      "type": "string",
      "default": "English"
    },
    "isFree": {
      "type": "boolean",
      "default": true
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating",
      "mappedBy": "driver"
    },
    "manufacture": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::manufacture.manufacture",
      "inversedBy": "drivers"
    },
    "downloadCount": {
      "type": "biginteger",
      "default": "0"
    },
    "driver_files": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::driver-file.driver-file",
      "mappedBy": "driver"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "drivers"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
